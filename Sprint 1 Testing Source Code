USER STORY 1
package UserStory1;

public class Junit {

public String compare(int Death_date, int Marriage_date) {
	// TODO Auto-generated method stub
	if (Death_date > Marriage_date)
		return ("Not Acceptable");
	else
	return ("Acceptable");
}

}
package UserStory1;

import static org.junit.Assert.*;

import org.junit.Test;

public class CompareTest {

	@Test1
	public void test() {
		Junit test = new Junit();
		String result = test.compare(20130202,20110212);
		assertEquals ("Not Acceptable",result);
		
	}

}

