package userStory2;

public class Junit {
	public String checking(int Birth_date,int Death_date)
	{
		if(Birth_date > Death_date)
			return ("Not Acceptable");
		else
			return ("Acceptable");
	}

}


package userStory2;

import static org.junit.Assert.*;

import org.junit.Test;

public class CheckingTest {

	@Test2
	public void test() {
		Junit test = new Junit();
		String result = test.checking(19931201,19921201);
		assertEquals("Not Acceptable",result);
	}

}
